<indexterm zone="glibtop-proc-open-files"><primary sortas="glibtop_proc_open_files">glibtop_proc_open_files</primary></indexterm><programlisting>typedef struct {
	guint64	flags;
	guint64 number;			/* GLIBTOP_PROC_OPEN_FILES_NUMBER */
	guint64 total;			/* GLIBTOP_PROC_OPEN_FILES_TOTAL  */
	guint64 size;			/* GLIBTOP_PROC_OPEN_FILES_SIZE   */
} glibtop_proc_open_files;
</programlisting>
