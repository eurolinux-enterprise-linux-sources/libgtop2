<refsynopsisdiv id="libgtop-PPP.synopsis" role="synopsis">
<title role="synopsis.title">Synopsis</title>

<synopsis>

#include &lt;glibtop/ppp.h&gt;

#define             <link linkend="GLIBTOP-PPP-STATE--CAPS">GLIBTOP_PPP_STATE</link>
#define             <link linkend="GLIBTOP-PPP-BYTES-IN--CAPS">GLIBTOP_PPP_BYTES_IN</link>
#define             <link linkend="GLIBTOP-PPP-BYTES-OUT--CAPS">GLIBTOP_PPP_BYTES_OUT</link>
#define             <link linkend="GLIBTOP-MAX-PPP--CAPS">GLIBTOP_MAX_PPP</link>
                    <link linkend="glibtop-ppp">glibtop_ppp</link>;
<link linkend="void">void</link>                <link linkend="glibtop-get-ppp">glibtop_get_ppp</link>                     (<link linkend="glibtop-ppp">glibtop_ppp</link> *buf,
                                                         <link linkend="unsigned-short-">unsigned short </link> device);
#define             <link linkend="glibtop-get-ppp-r">glibtop_get_ppp_r</link>
<link linkend="void">void</link>                <link linkend="glibtop-get-ppp-l">glibtop_get_ppp_l</link>                   (<link linkend="glibtop">glibtop</link> *server,
                                                         <link linkend="glibtop-ppp">glibtop_ppp</link> *buf,
                                                         <link linkend="unsigned-short-">unsigned short </link> device);
<link linkend="void">void</link>                <link linkend="glibtop-get-ppp-p">glibtop_get_ppp_p</link>                   (<link linkend="glibtop">glibtop</link> *server,
                                                         <link linkend="glibtop-ppp">glibtop_ppp</link> *buf,
                                                         <link linkend="unsigned-short-">unsigned short </link> device);
<link linkend="void">void</link>                <link linkend="glibtop-get-ppp-s">glibtop_get_ppp_s</link>                   (<link linkend="glibtop">glibtop</link> *server,
                                                         <link linkend="glibtop-ppp">glibtop_ppp</link> *buf,
                                                         <link linkend="unsigned-short-">unsigned short </link> device);
</synopsis>
</refsynopsisdiv>









