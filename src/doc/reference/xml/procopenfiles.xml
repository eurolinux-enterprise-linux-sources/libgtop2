<refsect2 id="glibtop-open-files-entry" role="struct">
<title>glibtop_open_files_entry</title>
<indexterm zone="glibtop-open-files-entry"><primary sortas="glibtop_open_files_entry">glibtop_open_files_entry</primary></indexterm><programlisting>typedef struct {
	int fd;
	guint16 type; /* An "enum glibtop_file_type" value. */
	union {
		/* When type == GLIBTOP_FILE_TYPE_INETSOCKET or
		 * when type == GLIBTOP_FILE_TYPE_INET6SOCKET */
		struct {
			char dest_host[GLIBTOP_OPEN_DEST_HOST_LEN+1];
			int dest_port;
		} sock;

		/* When type == GLIBTOP_FILE_TYPE_FILE */
		struct {
			char name[GLIBTOP_OPEN_FILENAME_LEN+1];
		} file;

		/* When type == GLIBTOP_FILE_TYPE_LOCALSOCKET */
		struct {
			char name[GLIBTOP_OPEN_FILENAME_LEN+1];
		} localsock;

	} info;
} glibtop_open_files_entry;
</programlisting>
<para>
</para></refsect2>
<refsect2 id="glibtop-proc-open-files" role="struct">
